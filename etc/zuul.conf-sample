[gearman]
server=127.0.0.1

[gearman_server]
start=true

[gerrit]
server=review.example.com
;baseurl=https://review.example.com/r
user=jenkins
sshkey=/home/jenkins/.ssh/id_rsa
;# Configure all proxy information (id, password, server ip or domain, and port).
;# Otherwise zuul will see it as nothing has been configured.
;proxy_id=your_proxy_id
;proxy_pw=your_proxy_pw
;proxy_server=your_proxy_server
;proxy_port=your_proxy_port

[zuul]
layout_config=/etc/zuul/layout.yaml
log_config=/etc/zuul/logging.conf
pidfile=/var/run/zuul/zuul.pid
state_dir=/var/lib/zuul
status_url=https://jenkins.example.com/zuul/status

[merger]
git_dir=/var/lib/zuul/git
;git_user_email=zuul@example.com
;git_user_name=zuul
zuul_url=http://zuul.example.com/p

[swift]
authurl=https://identity.api.example.org/v2.0/
user=username
key=password

default_container=logs
region_name=EXP
logserver_prefix=http://logs.example.org/server.app/

[smtp]
server=localhost
port=25
default_from=zuul@example.com
default_to=you@example.com
